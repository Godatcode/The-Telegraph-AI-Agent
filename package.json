{
  "name": "telegraph-ai-agent",
  "version": "1.0.0",
  "description": "A web-based application that resurrects 1844 Morse code protocol as a modern interface for communicating with AI",
  "type": "module",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "test": "vitest --run",
    "test:watch": "vitest",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "morse",
    "telegraph",
    "ai",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@vitejs/plugin-react": "^5.1.1",
    "fast-check": "^3.15.0",
    "jsdom": "^27.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supertest": "^7.1.4",
    "vitest": "^1.0.4"
  }
}
